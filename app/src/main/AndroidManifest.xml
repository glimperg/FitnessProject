<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.mprog.glimp.work_out">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WorkoutActivity" />
        <activity android:name=".CreateWorkoutActivity" />
        <activity android:name=".ChooseExerciseActivity" />
        <activity android:name=".WorkingOutActivity" />
        <activity android:name=".CreatePlannerActivity" />
        <activity android:name=".ExerciseActivity" />
        <activity android:name=".ChooseWorkoutActivity" />
    </application>

</manifest>